{% extends "layout.html.twig" %}

{% block title %}
    Home page
{% endblock %}

{% block css %}
    {{ parent() }}
{% endblock %}

{% block content %}
{% for key, player in players %}
    <p>Player{{ key+1}} is {{ player }}</p><br />
{% endfor %}
    {% for key, color in colors %}
        <p>Player{{ key+1}} color is {{ color }}</p><br />
    {% endfor %}
{% endblock %}